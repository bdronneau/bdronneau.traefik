# {{ ansible_managed }}

[entryPoints]
  [entryPoints.http]
  address = ":80"

  [entryPoints.https]
  address = ":443"

[certificatesResolvers]
  [certificatesResolvers.default.acme]
    email = "{{ btraefik_acme_email }}"
    storage = "/etc/traefik/acme/acme.json"

    [certificatesResolvers.default.acme.dnsChallenge]
      provider = "cloudflare"

[log]
  level = "WARN"

[metrics]
  [metrics.prometheus]
